<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Portafolio_XGVC</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Portafolio_XGVC.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <div class="nav-scroller py-1 mb-3 border-bottom">
            <nav class="nav nav-underline justify-content-between">
                <a class="nav-item nav-link link-body-emphasis active" href="#">Xana Velazquez</a>
                <a class="nav-item nav-link link-body-emphasis" href="#">INICIO</a>
                <a class="nav-item nav-link link-body-emphasis" href="#">SOBRE MI</a>
                <a class="nav-item nav-link link-body-emphasis" href="#">SKILLS</a>
                <a class="nav-item nav-link link-body-emphasis" href="#">PROYECTOS</a>
                <a class="nav-item nav-link link-body-emphasis" href="#">CONTACTO</a>
                <a class="nav-item nav-link link-body-emphasis" asp-area="" asp-controller="Youtube" asp-action="IndexY">YOUTUBE</a>
            </nav>
        </div>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - Portafolio_XGVC - <a asp-area="" asp-controller="Youtube" asp-action="IndexY">Api</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
